<Window x:Class="Lab2DPGI1.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab2DPGI1"
        mc:Ignorable="d"
        Title="Window3" Height="450" Width="800">
    <DockPanel LastChildFill="False">
        <Label DockPanel.Dock="Top" HorizontalAlignment="Center">Label</Label>
        <TextBox DockPanel.Dock="Left" Width="250" TextWrapping="Wrap">TextBox</TextBox>
        <TextBox DockPanel.Dock="Right" Width="250" TextWrapping="Wrap">TextBox</TextBox>
        <Button DockPanel.Dock="Bottom" Height="30" Width="50" Margin="10,0,10,30">Button</Button>
    </DockPanel>

</Window>
